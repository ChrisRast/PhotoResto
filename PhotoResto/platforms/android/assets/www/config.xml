<?xml version='1.0' encoding='utf-8' ?>
<widget id="ch.comem.photoresto" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PhotoResto</name>
    <description>
        Locate and rate restaurants with others and add pictures !
    </description>
    <author email="christophe.rast@heig-vd.ch" href="http://imchris.ch">
        Chris
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="fullscreen" value="false" />
    <preference name="Orientation" value="default" />
    <preference name="webviewbounce" value="true" />
    <preference name="KeepRunning" value="true" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="10000" />
        
