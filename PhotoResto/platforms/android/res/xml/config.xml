<?xml version='1.0' encoding='utf-8'?>
<widget id="ch.comem.photoresto" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello Cordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <name>PhotoResto</name>
    <description>
        Locate and rate restaurants with others and add pictures !
    </description>
    <name>PhotoResto</name>
    <description>
        Locate and rate restaurants with others and add pictures !
    </description>
    <author email="christophe.rast@heig-vd.ch" href="http://imchris.ch">
        Chris
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="fullscreen" value="false" />
    <preference name="Orientation" value="default" />
    <preference name="webviewbounce" value="true" />
    <preference name="KeepRunning" value="true" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="10000" />
</widget>
